type Result_1 = 
 variant {
   err: text;
   ok: DocSection;
 };
type Result = 
 variant {
   err: text;
   ok: nat;
 };
type DocSection = 
 record {
   content: text;
   id: nat;
   title: text;
 };
service : {
  getAllDocSections: () -> (vec DocSection) query;
  getDocSection: (nat) -> (Result_1) query;
  saveDocSection: (text, text) -> (Result);
}
